{
  "manifest_version": 2,

  "name": "Outbound Rules",
  "short_name": "outbound-rules",
  "description": "Protect your company's Admin Dashboards from XSS",
  "version": "0.1alpha3",
  "version_name": "0.1 alpha 3",

  
  "browser_action": {
    "default_icon": "logo.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
